<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.apl.lms.common.mapper.SpecialCommodityMapper">

    <!-- 查找特殊物品列表-->
    <select id="getList" resultType="com.apl.lms.common.query.manage.po.SpecialCommodityPo">
        SELECT
            id,
            special_commodity_name,
            special_commodity_name_en,
            code
        FROM
            common_special_commodity
    </select>

</mapper>
