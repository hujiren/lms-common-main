<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.apl.lms.common.mapper.CommonCarrierMapper">


    <!-- 查找列表 2020-10-07-->
    <select id="getList" resultType="com.apl.lms.common.query.manage.po.CommonCarrierPo">
        SELECT
           id, 
           carrier_name
        FROM
           common_carrier
        where
        1 = 1
        order by id desc
    </select>


</mapper>
