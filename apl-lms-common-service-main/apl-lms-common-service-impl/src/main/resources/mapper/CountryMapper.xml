<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.apl.lms.common.mapper.CountryMapper">



    <!-- 查找详情 2020-04-13-->
    <select id="getById" resultType="com.apl.lms.common.dto.CountryKeyDto">
        SELECT
          id, 
          country_code, 
          area_code, 
          name_cn, 
          name_en, 
          start_zip, 
          end_zip, 
          type
        FROM
          country
        where
          id = #{id}
    </select>

    <!-- 查找列表 2020-04-13-->
    <select id="getList" resultType="com.apl.lms.common.dto.CountryKeyDto">
        SELECT
           id, 
           country_code, 
           area_code, 
           name_cn, 
           name_en, 
           start_zip, 
           end_zip, 
           type
        FROM
           country
        where
        1 = 1
        order by id desc
    </select>


</mapper>
